oth values up to next thousand\r\ncurrentRPM = Math.ceil(currentRPM);\r\nmaxRPM = Math.ceil(maxRPM);\r\n\r\n// Calculate percentage (0-100)\r\nreturn Math.round((currentRPM / maxRPM) * 100 * 1000) / 1000;"},"Mode":2},"CircleGaugeColor":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"var currentRPM = $prop('DataCorePlugin.GameRawData.CurrentEngineRpm');\r\nvar redlineRPM = $prop('DataCorePlugin.GameData.CarSettings_RedLineRPM');\r\nvar maxRPM = $prop('DataCorePlugin.GameData.CarSettings_MaxRPM');\r\nvar currentTime = $prop('GameRawData.CurrentRaceTime');\r\n\r\n// Ensure valid values\r\nif (redlineRPM <= 0 || maxRPM <= 0) return \"#ffffff\";\r\n\r\n// Calculate the RPM at which blinking should start (75% between redline and max)\r\nvar blinkRPM = redlineRPM + ((maxRPM - redlineRPM) * 0.5);\r\n\r\n// High RPM blinking logic\r\nif (currentRPM >= blinkRPM) {\r\n    // Blink every 100ms (0.1 seconds)\r\n    return Math.floor(currentTime * 10) % 2 === 0 ? \"#ff0033\" : \"#ffffff\";\r\n}\r\n\r\n// Normal redline color change\r\nreturn currentRPM >= redlineRPM ? \"#ff0033\" : \"#ffffff\";"},"Mode":2}}},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.CircularGaugeItem, SimHub.Plugins","StrokeThickness":12.0,"Value":100.0,"Steps":0.0,"ValueEx":120.0,"MinValue":0.0,"MinAngle":0.0,"MaxValue":100.0,"MaxAngle":120.0,"CircleGaugeBackgroundColor":"#00000000","CircleGaugeColor":"#FFFF0033","BackgroundColor":"#00FFFFFF","Height":398.0,"Left":2.0,"Top":12.0,"Visible":true,"Width":398.0,"Name":"RedLine","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0,"Bindings":{"MinAngle":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"var engineMaxRpm = $prop('DataCorePlugin.GameRawData.EngineMaxRpm');\r\nvar carMaxRpm = $prop('DataCorePlugin.GameData.CarSettings_MaxRPM');\r\n\r\n// If either value is not yet available, return -120\r\nif (!engineMaxRpm || !carMaxRpm) {\r\n   return -120;\r\n}\r\n\r\n// Round up engineMaxRpm to next 1000\r\nvar roundedEngineMax = Math.ceil(engineMaxRpm / 1000) * 1000;\r\n\r\n// Calculate percentage (0-100)\r\nvar percentage = (carMaxRpm / roundedEngineMax) * 99.9;\r\n\r\n// Convert percentage to -120 to +120 scale\r\n// 0% = -120, 50% = 0, 100% = +120\r\nvar result = ((percentage - 50) * 240) / 100;\r\n\r\n// Ensure we stay within bounds\r\nreturn Math.max(-120, Math.min(120, result));"},"Mode":2}}},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.EllipseItem, SimHub.Plugins","FillColor":"#00FFFFFF","EllipseColor":"#00FFFFFF","EllipseThickness":0.0,"EllipseBackgroundImage":"RevLimit","Rotation":-120.0,"BackgroundColor":"#00FFFFFF","Height":398.0,"Left":2.0,"Top":12.0,"Visible":true,"Width":401.2,"Name":"RevLimit","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0,"Bindings":{"Rotation":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"var engineMaxRpm = $prop('DataCorePlugin.GameRawData.EngineMaxRpm');\r\nvar carMaxRpm = $prop('DataCorePlugin.GameData.CarSettings_MaxRPM');\r\n\r\n// If either value is not yet available, return -120\r\nif (!engineMaxRpm || !carMaxRpm) {\r\n   return -120;\r\n}\r\n\r\n// Round up engineMaxRpm to next 1000\r\nvar roundedEngineMax = Math.ceil(engineMaxRpm / 1000) * 1000;\r\n\r\n// Calculate percentage (0-100)\r\nvar percentage = (carMaxRpm / roundedEngineMax) * 100;\r\n\r\n// Convert percentage to rotation value\r\n// 50% = -120 rotation\r\n// 100% = 0 rotation\r\nvar rotation = -120 + ((percentage - 50) * 120) / 50;\r\n\r\n// Ensure we stay within bounds (-120 to 0) and round to 3 decimal places\r\nreturn Math.round(Math.max(-120, Math.min(0, rotation)) * 1000) / 1000;"},"Mode":2},"Width":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"var engineMaxRpm = $prop('DataCorePlugin.GameRawData.EngineMaxRpm');\r\nvar carMaxRpm = $prop('DataCorePlugin.GameData.CarSettings_MaxRPM');\r\n\r\n// Round up engineMaxRpm to next 1000\r\nvar roundedEngineMax = Math.ceil(engineMaxRpm / 1000) * 1000;\r\n\r\n// Calculate percentage (0-100)\r\nvar percentage = (carMaxRpm / roundedEngineMax) * 100;\r\n\r\n// Convert percentage to rotation value (50% = -120, 100% = 0)\r\nvar rotation = -120 + ((percentage - 50) * 120) / 50;\r\n\r\n// Ensure rotation stays within bounds (-120 to 0) and round to 3 decimals\r\nrotation = Math.round(Math.max(-120, Math.min(0, rotation)) * 1000) / 1000;\r\n\r\n// Mapping arrays (same as original)\r\nvar rotationPoints = [-120, -115, -110, -105, -100, -95, -90, -85, -80, -75, -70, -65, -60, -55, -45, -35, -25, -15, 0];\r\nvar widthPoints = [401.2, 400.8, 400.4, 400.1, 399.8, 399.6, 399.4, 399.3, 399.1, 399.0, 398.9, 398.7, 398.6, 398.5, 398.4, 398.3, 398.2, 398.1, 398.0];\r\n\r\n// Interpolation logic\r\nfor (var i = 0; i < rotationPoints.length - 1; i++) {\r\n    if (rotation >= rotationPoints[i] && rotation <= rotationPoints[i + 1]) {\r\n        var segmentProgress = (rotation - rotationPoints[i]) / (rotationPoints[i + 1] - rotationPoints[i]);\r\n        return Math.round((widthPoints[i] + (widthPoints[i + 1] - widthPoints[i]) * segmentProgress) * 1000) / 1000;\r\n    }\r\n}\r\n\r\n// Return default values if no match\r\nreturn rotation <= -120 ? 401.2 : 398.0;"},"Mode":2}}},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.EllipseItem, SimHub.Plugins","FillColor":"#00FFFFFF","EllipseColor":"#00FFFFFF","EllipseThickness":0.0,"EllipseBackgroundImage":"Markers-12k-RPM","BackgroundColor":"#00FFFFFF","Height":398.0,"Left":2.0,"Opacity":50.0,"Top":12.0,"Visible":true,"Width":398.0,"IsFreezed":true,"Name":"RPM-1k-Markers","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0,"Bindings":{"EllipseBackgroundImage":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"// Get the Max RPM value and round it up to the nearest 1000\r\nvar maxRPM = Math.ceil($prop('DataCorePlugin.GameData.MaxRpm') / 1000) * 1000;\r\n\r\n// Determine the corresponding marker\r\nif (maxRPM <= 2000) return \"Markers-2k-RPM\";\r\nif (maxRPM <= 3000) return \"Markers-3k-RPM\";\r\nif (maxRPM <= 4000) return \"Markers-4k-RPM\";\r\nif (maxRPM <= 5000) r