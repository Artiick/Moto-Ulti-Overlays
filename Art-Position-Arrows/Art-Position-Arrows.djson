{"Variables":{"DashboardVariables":[{"VariableName":"posGainLoss","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":false,"ValueExpression":{"JSExt":0,"Interpreter":1,"Expression":"//Artiick-ver 1.5.9: Position gained/loss calc + colors etc\r\n\r\nvar dist = $prop(\"GameRawData.DistanceTraveled\");\r\nvar pos = $prop(\"Position\");\r\nvar currentLap = $prop(\"GameRawData.CurrentLap\");\r\nvar completedLaps = $prop(\"CompletedLaps\");\r\nvar startFlag = \"posGainLoss_startPos\";\r\n\r\n// Reset logic\r\nif (currentLap === 0 && completedLaps === 0) {\r\n    delete globalThis[startFlag];\r\n}\r\n\r\n// Store starting position\r\nif (typeof globalThis[startFlag] === \"undefined\" && dist > -1600) {\r\n    globalThis[startFlag] = pos;\r\n}\r\n\r\nvar startPos = globalThis[startFlag];\r\nvar gainLoss = 0;\r\nvar imageGroup = \"pos-neutr\";\r\nvar digit1 = \"\";\r\nvar digit2 = \"\";\r\nvar visP1 = \"0\", visP2 = \"0\", visP3 = \"0\";\r\nvar color = \"\";\r\n\r\nif (typeof startPos !== \"undefined\") {\r\n    gainLoss = startPos - pos;\r\n\r\n    if (gainLoss > 0) {\r\n        imageGroup = \"pos-gain\";\r\n        color = \"#00ff7f\";\r\n    } else if (gainLoss < 0) {\r\n        imageGroup = \"pos-loss\";\r\n        color = \"#ff0033\";\r\n    }\r\n\r\n    var absDiff = Math.abs(gainLoss);\r\n    if (absDiff > 0 && absDiff < 10) {\r\n        digit1 = absDiff.toString();\r\n    } else if (absDiff >= 10) {\r\n        digit2 = absDiff.toString();\r\n    }\r\n\r\n    if (pos === 1) {\r\n        visP1 = \"1\";\r\n    } else if (pos === 2) {\r\n        visP2 = \"1\";\r\n    } else {\r\n        visP3 = \"1\";\r\n    }\r\n}\r\n\r\nvar result = \"\";\r\nresult += \"0.\" + imageGroup + \" \";\r\nresult += \"1.\" + imageGroup + \" \";\r\nresult += \"2.\" + imageGroup + \" \";\r\n\r\nresult += \"3.\" + (visP1 === \"1\" ? digit1 : \"\") + \" \";\r\nresult += \"4.\" + (visP2 === \"1\" ? digit1 : \"\") + \" \";\r\nresult += \"5.\" + (visP3 === \"1\" ? digit1 : \"\") + \" \";\r\n\r\nresult += \"6.\" + (visP1 === \"1\" ? digit2 : \"\") + \" \";\r\nresult += \"7.\" + (visP2 === \"1\" ? digit2 : \"\") + \" \";\r\nresult += \"8.\" + (visP3 === \"1\" ? digit2 : \"\") + \" \";\r\n\r\nresult += \"9.\" + visP1 + \" \";\r\nresult += \"10.\" + visP2 + \" \";\r\nresult += \"11.\" + visP3 + \" \";\r\n\r\nresult += \"12.\" + (visP1 === \"1\" ? color : \"\") + \" \";\r\nresult += \"13.\" + (visP2 === \"1\" ? color : \"\") + \" \";\r\nresult += \"14.\" + (visP3 === \"1\" ? color : \"\");\r\n\r\nreturn result.trim();\r\n"},"EvaluateBeforeScreenRoles":false}]},"DashboardDebugManager":{},"Version":2,"Id":"4b1ed067-ee1d-4e15-b3bf-e89013c2ea16","BaseHeight":212,"BaseWidth":100,"BackgroundColor":"#FF000000","Screens":[{"RenderingSkip":0,"Name":"Screen","InGameScreen":true,"IdleScreen":true,"PitScreen":false,"ScreenId":"e2605d17-a83b-40d5-a928-64e51fbf585c","AllowOverlays":true,"IsForegroundLayer":false,"IsOverlayLayer":false,"OverlayTriggerExpression":{"Expression":""},"ScreenEnabledExpression":{"Expression":""},"OverlayMaxDuration":0,"OverlayMinDuration":0,"IsBackgroundLayer":false,"BackgroundColor":"#00FFFFFF","Items":[{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.ImageItem, SimHub.Plugins","Image":"text1440","AutoSize":false,"BackgroundColor":"#00FFFFFF","Height":212.0,"Left":0.0,"Top":0.0,"Visible":false,"Width":20.0,"Name":"ImageItem","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.Layer, SimHub.Plugins","Group":true,"Repetitions":0,"Childrens":[{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins","IsTextItem":true,"ShadowColor":"#A1000000","Font":"ForzaFont FuelTire-Num-SB","FontSize":25.0,"Text":"4","TextColor":"#FFFFFFFF","HorizontalAlignment":1,"VerticalAlignment":1,"UseMonospacedText":true,"CharWidth":17.5,"BackgroundColor":"#00FFFFFF","Height":27.0,"Left":1.5,"Top":118.5,"Visible":true,"Width":17.0,"Name":"P3","RenderingSkip":