{"Variables":{"DashboardVariables":[{"VariableName":"predictedPitStop","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":false,"ValueExpression":{"JSExt":0,"Interpreter":1,"Expression":"// predictedPitStop-Ver 1.5.5: Predict pit stop laps left and reason, recalculate on lap completion\r\n\r\n// Read telemetry data\r\nvar trackId = parseInt($prop(\"TrackId\")) || -1;\r\nvar carId = parseInt($prop(\"CarId\")) || -1;\r\nvar distanceTraveled = parseFloat($prop(\"GameRawData.DistanceTraveled\")) || 0;\r\nvar completedLaps = parseInt($prop(\"CompletedLaps\")) || 0;\r\nvar trackLength = parseFloat($prop(\"TrackLength\")) || 0;\r\nvar fuelPercent = parseFloat($prop(\"FuelPercent\")) || 0;\r\n\r\n// Tire wear inputs (last lap and current)\r\nvar lastLapWearFL = parseFloat(getsharedvalueordefault(\"artiick\", \"lastLapWearFL\", \"0\")) || 0;\r\nvar lastLapWearFR = parseFloat(getsharedvalueordefault(\"artiick\", \"lastLapWearFR\", \"0\")) || 0;\r\nvar lastLapWearRL = parseFloat(getsharedvalueordefault(\"artiick\", \"lastLapWearRL\", \"0\")) || 0;\r\nvar lastLapWearRR = parseFloat(getsharedvalueordefault(\"artiick\", \"lastLapWearRR\", \"0\")) || 0;\r\nvar currentWearFL = (parseFloat($prop(\"GameRawData.TireWearFrontLeft\")) || 0) * 100;\r\nvar currentWearFR = (parseFloat($prop(\"GameRawData.TireWearFrontRight\")) || 0) * 100;\r\nvar currentWearRL = (parseFloat($prop(\"GameRawData.TireWearRearLeft\")) || 0) * 100;\r\nvar currentWearRR = (parseFloat($prop(\"GameRawData.TireWearRearRight\")) || 0) * 100;\r\n\r\n// Fuel input\r\nvar lapsEstFuelLeft = parseFloat(getsharedvalueordefault(\"artiick\", \"lapsEstFuelLeft\", \"0\")) || 0;\r\n\r\n// Initialize persistent storage\r\nif (typeof globalThis.predictedPitStopData === 'undefined') {\r\n    globalThis.predictedPitStopData = {\r\n        trackId: -1,\r\n        carId: -1,\r\n        lastCompletedLaps: -1,\r\n        pitLap: null,\r\n        lastResetTime: 0,\r\n        prevWearFL: 0,\r\n        prevWearFR: 0,\r\n        prevWearRL: 0,\r\n        prevWearRR: 0,\r\n        prevFuel: 0\r\n    };\r\n}\r\n\r\n// Reset on track/car change or session reset\r\nif ((trackId !== globalThis.predictedPitStopData.trackId || carId !== globalThis.predictedPitStopData.carId || \r\n     (distanceTraveled < 0 && completedLaps === 0)) && Date.now() - globalThis.predictedPitStopData.lastResetTime > 1000) {\r\n    globalThis.predictedPitStopData = {\r\n        trackId: trackId,\r\n        carId: carId,\r\n        lastCompletedLaps: -1,\r\n        pitLap: null,\r\n        lastResetTime: Date.now(),\r\n        prevWearFL: currentWearFL,\r\n        prevWearFR: currentWearFR,\r\n        prevWearRL: currentWearRL,\r\n        prevWearRR: currentWearRR,\r\n        prevFuel: fuelPercent\r\n    };\r\n}\r\n\r\n// Detect pit stop (tire wear decrease or fuel increase)\r\nvar pitStopDetected = false;\r\nif (currentWearFL < globalThis.predictedPitStopData.prevWearFL - 5 || \r\n    currentWearFR < globalThis.predictedPitStopData.prevWearFR - 5 || \r\n    currentWearRL < globalThis.predictedPitStopData.prevWearRL - 5 || \r\n    currentWearRR < globalThis.predictedPitStopData.prevWearRR - 5 || \r\n    fuelPercent > globalThis.predictedPitStopData.prevFuel + 5) {\r\n    pitStopDetected = true;\r\n    globalThis.predictedPitStopData.pitLap = null;\r\n}\r\n\r\n// Detect new lap completion\r\nif (completedLaps > globalThis.predictedPitStopData.lastCompletedLaps) {\r\n    globalThis.predictedPitStopData.pitLap = null; // Force recalculation\r\n    globalThis.predictedPitStopData.lastCompletedLaps = completedLaps;\r\n}\r\n\r\n// Update previous values\r\nglobalThis.predictedPitStopData.prevWearFL = currentWearFL;\r\nglobalThis.predictedPitStopData.prevWearFR = currentWearFR;\r\nglobalThis.predictedPitStopData.prevWearRL = currentWearRL;\r\nglobalThis.predictedPitStopData.prevWearRR = currentWearRR;\r\nglobalThis.predictedPitStopData.prevFuel = fuelPercent;\r\n\r\n// Calculate laps left for each tire (max wear