{"Variables":{"DashboardVariables":[{"VariableName":"AvgFuel","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":false,"ValueExpression":{"JSExt":0,"Interpreter":1,"Expression":"// Artiick-Ver 1.5.28: Fuel tracking AVG calc \r\n\r\n// Fetch required properties\r\nvar CarId = $prop(\"CarId\");\r\nvar TrackId = $prop(\"TrackId\");\r\nvar CompletedLaps = $prop(\"CompletedLaps\");\r\nvar DistanceTraveled = $prop('GameRawData.DistanceTraveled');\r\nvar FuelPercent = $prop(\"FuelPercent\");\r\n\r\n// Initialize global storage\r\nif (typeof globalThis.FuelSnapTracker === 'undefined') {\r\n    globalThis.FuelSnapTracker = {\r\n        lastCarId: CarId,\r\n        lastTrackId: TrackId,\r\n        snaps: [],\r\n        lastLap: 0,\r\n        snapTaken: false,\r\n        lastValidFuel: 0,\r\n        lastValidResult: 0  // New: store last valid average consumption\r\n    };\r\n}\r\n\r\n// HARD RESET if car or track changes\r\nif (globalThis.FuelSnapTracker.lastCarId !== CarId || globalThis.FuelSnapTracker.lastTrackId !== TrackId) {\r\n    globalThis.FuelSnapTracker.lastCarId = CarId;\r\n    globalThis.FuelSnapTracker.lastTrackId = TrackId;\r\n    globalThis.FuelSnapTracker.snaps = [];\r\n    globalThis.FuelSnapTracker.lastLap = 0;\r\n    globalThis.FuelSnapTracker.snapTaken = false;\r\n    globalThis.FuelSnapTracker.lastValidFuel = 0;\r\n    globalThis.FuelSnapTracker.lastValidResult = 0;\r\n}\r\n\r\n// SAFETY: Track last valid fuel\r\nif (FuelPercent > 0) {\r\n    globalThis.FuelSnapTracker.lastValidFuel = FuelPercent;\r\n} else {\r\n    FuelPercent = globalThis.FuelSnapTracker.lastValidFuel || 0;\r\n}\r\n\r\n// SNAP1: When moving for first time\r\nif (!globalThis.FuelSnapTracker.snapTaken && CompletedLaps === 0 && DistanceTraveled > 0.2 && FuelPercent > 0) {\r\n    globalThis.FuelSnapTracker.snaps = [FuelPercent];\r\n    globalThis.FuelSnapTracker.snapTaken = true;\r\n}\r\n\r\n// SNAP2, SNAP3 and beyond: Each lap completion, sliding window\r\nif (CompletedLaps > globalThis.FuelSnapTracker.lastLap && CompletedLaps > 0 && FuelPercent > 0) {\r\n    var snaps = globalThis.FuelSnapTracker.snaps;\r\n\r\n    // Refuel detection: If fuel increased, reset snapshots\r\n    if (snaps.length > 0 && FuelPercent > snaps[snaps.length - 1] + 1) {\r\n        snaps.length = 0;\r\n        snaps.push(FuelPercent);\r\n        globalThis.FuelSnapTracker.lastLap = CompletedLaps;\r\n\r\n        // Return last valid result instead of zero\r\n        return globalThis.FuelSnapTracker.lastValidResult.toFixed(2);\r\n    }\r\n\r\n    // Maintain rolling last 3 snaps\r\n    if (snaps.length >= 3) {\r\n        snaps.shift();\r\n    }\r\n    snaps.push(FuelPercent);\r\n\r\n    globalThis.FuelSnapTracker.lastLap = CompletedLaps;\r\n}\r\n\r\n// If not enough snapshots yet, return last valid result\r\nif (globalThis.FuelSnapTracker.snaps.length < 2) {\r\n    return globalThis.FuelSnapTracker.lastValidResult.toFixed(2);\r\n}\r\n\r\n// Calculation\r\nvar snaps = globalThis.FuelSnapTracker.snaps;\r\nvar diffs = [];\r\nvar validDiffs = true;\r\n\r\nfor (var i = 1; i < snaps.length; i++) {\r\n    var diff = snaps[i - 1] - snaps[i];\r\n    if (diff < 0 || snaps[i] === 0) {\r\n        validDiffs = false;\r\n        break;\r\n    }\r\n    diffs.push(diff);\r\n}\r\n\r\nvar result = 0;\r\n\r\nif (validDiffs && diffs.length >= 2) {\r\n    result = (diffs[0] + diffs[1]) / 2;\r\n    globalThis.FuelSnapTracker.lastValidResult = result;  // Update cached valid result\r\n} else if (diffs.length >= 1) {\r\n    result = diffs[diffs.length - 1];\r\n    globalThis.FuelSnapTracker.lastValidResult = result;  // Update cached valid result\r\n} else {\r\n    result = globalThis.FuelSnapTracker.lastValidResult;  // Use cached valid result\r\n}\r\n\r\nreturn result.toFixed(2);\r\n"},"EvaluateBeforeScreenRoles":false},{"VariableName":"EstFuel","EvaluateOnlyOnce":false,"OverrideWithParentDashboardVariableWhenAvailable":false,"ValueExpression":{"JSExt":0,"Interpreter":1,"Expression":"//Artiick-Ver 1.5.0: Estimated Fuel (in laps)\r\n\r\n// Read telemetry data\r\nvar avgFuelPerLap = $prop('variable.AvgFuel');\r\nvar currentFuelPercent = $prop('FuelPercent');\r\n\r\n// Check for valid values to avoid division by zero or invalid results\r\nif (avgFuelPerLap > 0.0000001 && currentFuelPercent >= 0) {\r\n    // Calculate estimated laps remaining\r\n    var lapsRemaining = currentFuelPercent / avgFuelPerLap;\r\n    return lapsRemaining;\r\n}\r\n\r\n// Default return if invalid (e.g., avgFuelPerLap is 0 or FuelPercent is negative)\r\nreturn 0;"},"EvaluateBeforeScreenRoles":false}]},"DashboardDebugManager":{},"Version":2,"Id":"ef57e9b0-ef9a-472a-8a31-61f7338e03e5","BaseHeight":217,"BaseWidth":115,"BackgroundColor":"#FF000000","Screens":[{"RenderingSkip":0,"Name":"Screen","InGameScreen":true,"IdleScreen":true,"PitScreen":false,"ScreenId":"8ac7d498-e1f0-4275-85cb-7726aa3fed83","AllowOverlays":true,"IsForegroundLayer":false,"IsOverlayLayer":false,"OverlayTriggerExpression":{"Expression":""},"ScreenEnabledExpression":{"Expression":""},"OverlayMaxDuration":0,"OverlayMinDuration":0,"IsBackgroundLayer":false,"BackgroundColor":"#00FFFFFF","Items":[{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.ImageItem, SimHub.Plugins","AutoSize":false,"BackgroundColor":"#00FFFFFF","Height":217.0,"Left":-1.5,"Top":0.0,"Visible":true,"Width":110.0,"IsFreezed":true,"Name":"Align","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0,"Bindings":{"Top":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"setsharedvalue(\"artiick\", \"lapsEstFuelLeft\", $prop(\"variable.EstFuel\"))"},"Mode":2}}},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.Layer, SimHub.Plugins","Group":true,"Repetitions":0,"Childrens":[{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.ImageItem, SimHub.Plugins","Image":"Fuel-Icon","AutoSize":false,"BackgroundColor":"#00FFFFFF","Height":50.0,"Left":25.6,"Top":162.7,"Visible":true,"Width":45.0,"IsFreezed":true,"Name":"FuelIco"