etedLaps = $prop('CompletedLaps');\r\nvar FuelPercent = $prop('DataCorePlugin.Computed.Fuel_Percent');\r\nvar SessionOdo = $prop('DataCorePlugin.GameData.SessionOdo');\r\nvar RaceTime = $prop('GameRawData.CurrentRaceTime');\r\n\r\n// Initialize global storage if needed\r\nif (typeof globalThis.FuelTracker === 'undefined') {\r\n    globalThis.FuelTracker = {\r\n        lastFuel: null,       // Stores last recorded fuel percentage\r\n        fuelDifferences: [],  // Stores last 3 fuel consumption values\r\n        initialized: false,    // Tracks if first fuel value has been set\r\n        lastLap: 0            // Stores last completed lap\r\n    };\r\n}\r\n\r\n// Prevent reset if session has already started (ignore post-pause reset cases)\r\nif ((SessionOdo < 0.1 || RaceTime < 2) && !globalThis.FuelTracker.initialized) {\r\n    globalThis.FuelTracker = {\r\n        lastFuel: null,\r\n        fuelDifferences: [],\r\n        initialized: false,\r\n        lastLap: 0\r\n    };\r\n    return \"--.-\";\r\n}\r\n\r\n// Store the first valid fuel value once session is properly started\r\nif (!globalThis.FuelTracker.initialized && SessionOdo > 0.1 && RaceTime > 2 && FuelPercent > 0) {\r\n    globalThis.FuelTracker.lastFuel = FuelPercent;\r\n    globalThis.FuelTracker.initialized = true;\r\n}\r\n\r\n// Only track fuel if it's a valid value (ignore 0)\r\nif (FuelPercent > 0) {\r\n    // Store fuel data when a lap is completed\r\n    if (CompletedLaps > globalThis.FuelTracker.lastLap) {\r\n        if (globalThis.FuelTracker.lastFuel !== null) {\r\n            var fuelDiff = globalThis.FuelTracker.lastFuel - FuelPercent;\r\n\r\n            // Ignore refueling laps (fuelDiff should be positive)\r\n            if (fuelDiff > 0) {\r\n                globalThis.FuelTracker.fuelDifferences.unshift(fuelDiff);\r\n\r\n                // Keep only the last 3 values\r\n                if (globalThis.FuelTracker.fuelDifferences.length > 3) {\r\n                    globalThis.FuelTracker.fuelDifferences.pop();\r\n                }\r\n            }\r\n        }\r\n\r\n        // Update last stored fuel and lap number\r\n        globalThis.FuelTracker.lastFuel = FuelPercent;\r\n        globalThis.FuelTracker.lastLap = CompletedLaps;\r\n    }\r\n}\r\n\r\n// If no laps have been completed yet, return last known fuel consumption (instead of \"--.-\")\r\nif (CompletedLaps === 0) {\r\n    return globalThis.FuelTracker.fuelDifferences.length > 0\r\n        ? globalThis.FuelTracker.fuelDifferences[0].toFixed(1) + '%' \r\n        : \"--.-\";\r\n}\r\n\r\n// Calculate the average fuel consumption\r\nif (globalThis.FuelTracker.fuelDifferences.length > 0) {\r\n    var avgFuelConsumption = globalThis.FuelTracker.fuelDifferences.reduce((a, b) => a + b, 0) / globalThis.FuelTracker.fuelDifferences.length;\r\n    return avgFuelConsumption.toFixed(1) + '%';\r\n}\r\n\r\n// If no valid fuel data yet, return last known value instead of resetting\r\nreturn globalThis.FuelTracker.lastFuel !== null ? globalThis.FuelTracker.lastFuel.toFixed(1) + '%' : \"--.-\";"},"Mode":2}}}],"Visible":true,"Name":"AVG","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.Layer, SimHub.Plugins","Group":true,"Repetitions":0,"IsFreezed":true,"Childrens":[{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins","IsTextItem":true,"ShadowDepth":1,"ShadowColor":"#A1000000","Font":"ForzaFont Text","FontSize":20.0,"Text":"FUEL","TextColor":"#FFBFBFBF","HorizontalAlignment":0,"VerticalAlignment":0,"BackgroundColor":"#00FFFFFF","Height":29.0,"Left":25.5,"Top":104.8,"Visible":true,"Width":45.0,"Name":"Fuel","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.TextItem, SimHub.Plugins","IsTextItem":true,"ShadowDepth":2,"ShadowColor":"#A1000000","Font":"ForzaFont FuelTire-Num-R","FontSize":35.0,"Text":"44.4%","TextColor":"#FFBFBFBF","HorizontalAlignment":0,"VerticalAl