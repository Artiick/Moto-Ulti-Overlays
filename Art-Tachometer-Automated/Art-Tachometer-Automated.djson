\nif (typeof globalThis.maxRPMPercent === 'undefined') {\r\n    globalThis.maxRPMPercent = 0;\r\n    globalThis.oneShot = true;\r\n    globalThis.lastCarId = currentCarId;\r\n}\r\n\r\n// Check for CarId change → Hard reset\r\nif (currentCarId !== globalThis.lastCarId) {\r\n    globalThis.maxRPMPercent = 0;\r\n    globalThis.oneShot = true;\r\n    globalThis.lastCarId = currentCarId;\r\n}\r\n\r\n// After 5s, stop tracking and just return stored max value\r\nif (raceTime > 10) currentRPMPercent = globalThis.maxRPMPercent;\r\n\r\n// Update max RPM%\r\nif (currentRPMPercent > globalThis.maxRPMPercent || globalThis.oneShot === true) {\r\n    globalThis.maxRPMPercent = currentRPMPercent;\r\n    globalThis.oneShot = false;\r\n} else if (currentRPMPercent < 5) {\r\n    globalThis.maxRPMPercent = 0;\r\n}\r\n\r\n// Calculate width using max RPM%\r\nreturn -120.1 + (globalThis.maxRPMPercent * 2.4);"},"Mode":2}}},{"$type":"SimHub.Plugins.OutputPlugins.GraphicalDash.Models.EllipseItem, SimHub.Plugins","FillColor":"#00FFFFFF","EllipseColor":"#00FFFFFF","EllipseThickness":0.0,"EllipseBackgroundImage":"RevLimit","Rotation":-120.0,"BackgroundColor":"#00FFFFFF","Height":398.0,"Left":2.0,"Top":12.0,"Visible":true,"Width":401.2,"Name":"RevLimit","RenderingSkip":0,"MinimumRefreshIntervalMS":0.0,"Bindings":{"Rotation":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"var currentRPMPercent = $prop('DataCorePlugin.GameData.CarSettings_CurrentDisplayedRPMPercent');\r\nvar currentCarId = $prop('DataCorePlugin.GameData.CarId');\r\nvar raceTime = $prop('DataCorePlugin.GameRawData.CurrentRaceTime');\r\n\r\n// Initialize storage\r\nif (typeof globalThis.maxRPMPercent === 'undefined') {\r\n    globalThis.maxRPMPercent = 0;\r\n    globalThis.oneShot = true;\r\n    globalThis.lastCarId = currentCarId;\r\n}\r\n\r\n// Check for CarId change → Hard reset\r\nif (currentCarId !== globalThis.lastCarId) {\r\n    globalThis.maxRPMPercent = 0;\r\n    globalThis.oneShot = true;\r\n    globalThis.lastCarId = currentCarId;\r\n}\r\n\r\n// After 5s, stop tracking and just return stored max value\r\nif (raceTime > 10) currentRPMPercent = globalThis.maxRPMPercent;\r\n\r\n// Update max RPM%\r\nif (currentRPMPercent > globalThis.maxRPMPercent || globalThis.oneShot === true) {\r\n    globalThis.maxRPMPercent = currentRPMPercent;\r\n    globalThis.oneShot = false;\r\n} else if (currentRPMPercent < 5) {\r\n    globalThis.maxRPMPercent = 0;\r\n}\r\n\r\n// Then calculate rotation based on stored max\r\nif (globalThis.maxRPMPercent < 50) return -120;\r\nif (globalThis.maxRPMPercent > 100) return 0;\r\n\r\nreturn -120 + (120 * (globalThis.maxRPMPercent - 50) / 50);"},"Mode":2},"Width":{"Formula":{"JSExt":0,"Interpreter":1,"Expression":"var currentRPMPercent = $prop('DataCorePlugin.GameData.CarSettings_CurrentDisplayedRPMPercent');\r\nvar currentCarId = $prop('DataCorePlugin.GameData.CarId');\r\nvar raceTime = $prop('DataCorePlugin.GameRawData.CurrentRaceTime');\r\n\r\n// Initialize storage\r\nif (typeof globalThis.maxRPMPercent === 'undefined') {\r\n    globalThis.maxRPMPercent = 0;\r\n    globalThis.oneShot = true;\r\n    globalThis.lastCarId = currentCarId;\r\n}\r\n\r\n// Reset when car changes\r\nif (currentCarId !== globalThis.lastCarId) {\r\n    globalThis.maxRPMPercent = 0;\r\n    globalThis.oneShot = true;\r\n    globalThis.lastCarId = currentCarId;\r\n}\r\n\r\n// After 10s, stop updating maxRPMPercent\r\nif (raceTime > 10) currentRPMPercent = globalThis.maxRPMPercent;\r\n\r\n// Update max RPM%, but don't reset if it drops below 5\r\nif (currentRPMPercent > globalThis.maxRPMPercent || globalThis.oneShot === true) {\r\n    globalThis.maxRPMPercent = currentRPMPercent;\r\n    globalThis.oneShot = false;\r\n}\r\n\r\n// Calculate rotation\r\nvar rotation = -120;\r\nif (globalThis.maxRPMPercent >= 50 && globalThis.maxRPMPercent <= 100) {\r\n    rotation = -120 + (120 * (globalThis.maxRPMPercent - 50) / 50);\r\n} else if (globalThis.maxRPMPercent > 100) {\r\n    rotation = 0;\r\n}